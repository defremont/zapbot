{
  "name": "Zap Save Contacts",
  "version": "1.0",
  "description":
    "This extension enable a button in whatsapp web, that open the selected contact details and copy name + numer to clipboard in google contacts format for .csv importation.",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/"],
      "js": ["zap.js"]
    }
  ],
  "page_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
